{
  "_default_": {
    "_all": {
      "enabled": false
    },
    "_source": {
      "enabled": true
    },
    "date_detection": false,
    "numeric_detection": false,
    "properties": {
      "displayname._ngram": {
        "type": "string",
        "store": "no",
        "index": "analyzed",
        "analyzer": "ngram_index",
        "include_in_all": false
      }
    },
    "dynamic_templates": [
      {
        "template_fulltext": {
          "match": "*._analyzed",
          "mapping": {
            "type": "string",
            "store": "no",
            "index": "analyzed",
            "analyzer": "document_index_default",
            "include_in_all": false
          }
        }
      },
      {
        "template_path": {
          "match": "*._path",
          "mapping": {
            "type": "string",
            "store": "no",
            "index": "analyzed",
            "analyzer": "path_analyzer",
            "include_in_all": false
          }
        }
      },
      {
        "template_ngram": {
          "match": "*._ngram",
          "mapping": {
            "type": "string",
            "store": "no",
            "index": "analyzed",
            "analyzer": "ngram_index_front",
            "search_analyzer": "document_index_default",
            "include_in_all": false
          }
        }
      },
      {
        "template_userdata_date": {
          "match": "*._datetime",
          "mapping": {
            "type": "date",
            "store": "no",
            "index": "not_analyzed",
            "include_in_all": false
          }
        }
      },
      {
        "template_userdata_number": {
          "match": "*._number",
          "mapping": {
            "type": "double",
            "store": "no",
            "index": "not_analyzed",
            "include_in_all": false
          }
        }
